@model List<LicenceCompare>

<div class="card">
    <div class="card-header bg-gradient-teal">
        <h3 class="card-title">Clone And Compare Licence</h3>
    </div>
    <div class="card-body">
        <table id="feature_list" class="table table-bordered dt-responsive nowrap">
            <thead>
                <tr>
                    
                    <th>RequisitionNo</th>
                    <th>Product Name</th>
                    <th>Compare</th>
                    <th>Clone</th>
                </tr>
            </thead>

            <tbody>

                @foreach (LicenceCompare PF in Model)
                {
                <tr>
                    @*<td><input type="radio" name="requisitionId" value="@PF.RequestNo" /></td>*@
                    <td>@PF.RequestNo</td>
                    <td>@PF.ProductName</td>
                    <td><a href="@Url.Action("DisplayCompareLicence","Requisition", new { id = PF.RequestNo })" target="_blank" class="btn btn-primary" onclick="stopmodal();"> Compare Licence </a></td>
                    <td><a href="@Url.Action("Index","Requisition", new { id = PF.RequestNo })" class="btn btn-primary"  onclick="stopmodal();"> Clone Licence </a></td>
                </tr>
                }

            </tbody>
        </table>
        <br />
        @*<div style="align-content:center">
            <button type="submit" id="" name="Compare" class="btn btn-success"><i class="fas fa-save"></i> Compare</button>
        </div>*@
       
    </div>
</div>
<script>
    function stopmodal()
        {
        $("#myModal").modal("hide");
    }
</script>