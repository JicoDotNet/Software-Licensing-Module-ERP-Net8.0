@model List<ReportOfMakerChecker>

@if (Model.Count != 0)
{

    <table id="MakerChecker_View" class="table table-striped table-bordered dt-responsive nowrap">
        <thead>
            <tr>
                <th>Case Type</th>
                <th>Purpose</th>
                <th>CreatedUser</th>
                <th>CreatedUserType</th>
                <th>Approve Status</th>
                <th>ApproveRejectUser</th>
                <th>ApproveRejectUserType</th>
                <th>ApproveRejectRemarks</th>
                <th>ApproveRejectDate</th>
                <th>Activity Status</th>
                <th>Request Date</th>
            </tr>
        </thead>
        <tbody>
            @foreach (ReportOfMakerChecker reportOfMakerChecker in Model)
            {
            <tr>
                <td>@reportOfMakerChecker.CaseType</td>
                <td>@reportOfMakerChecker.Purpose</td>
                <td>@reportOfMakerChecker.FullName</td>
                <td>@reportOfMakerChecker.UserTypeName</td>
                <td>@reportOfMakerChecker.IsApproved</td>

                <td>@reportOfMakerChecker.FullName</td>
                <td>@reportOfMakerChecker.UserTypeName</td>


                <td>@reportOfMakerChecker.ApproveRejectRemarks</td>
                <td>@reportOfMakerChecker.ApproveRejectDate</td>
                <td>@reportOfMakerChecker.IsActive</td>
                <td>@reportOfMakerChecker.TransactionDate</td>
            </tr>
            }

        </tbody>
    </table>
}
else
{
    <script type="text/javascript">
        toastr.error('No data Found');
    </script>

}
<script type="text/javascript">
    $(document).ready(function () {
        $('#MakerChecker_View').DataTable({
            dom: 'lBfrtip',
            buttons: [
                'copy', 'csv', 'excel', 'pdf', 'print'
            ],
            "responsive": true,
        });
    });

    $('.toast').toast('show');
</script>